public class Worker {

    String name;
    int age;

    static int count = 0;

    public static void main(String[] args) {
        Worker Liza = new Worker();
        Lize.age = 19;
        Lize.name = "Лиза";
        count++;

        Worker Kate = new Worker();
        Kate.age = 21;
        Kate.name = "Катя";
        count++;
        
        Worker Roma = new Worker();
        Roma.age = 25;
        Roma.name = "Рома";
        count++;
        
        Worker Gleb = new Worker();
        Gleb.age = 20;
        Gleb.name = "Глеб";
        count++;

        System.out.println("Сотрудник по имени " + Liza.name + ", его возраст - " + Liza.age);
        System.out.println("Сотрудник по имени " + Kate.name + ", его возраст - " + Kate.age);
        System.out.println("Сотрудник по имени " + Roma.name + ", его возраст - " + Roma.age);
        System.out.println("Сотрудник по имени " + Gleb.name + ", его возраст - " + Gleb.age);

        System.out.println("Общее количество сотрудников = " + count);
    }
}
